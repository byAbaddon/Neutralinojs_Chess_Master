{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "window.*",
    "debug.log"
  ],
  "globalVariables": {
    "START": true
  },
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "modes": {
    "window": {
      "title": "Chess Master",
      "width": 800,
      "height": 640,
      "minWidth": 800,
      "minHeight": 640,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/appIcon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "minimize": true,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": true
    },
    "tokenSecurity": "one-time"
  },
  "cli": {
    "binaryName": "myapp",
    "resourcesPath": "/resources",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "5.5.0",
    "clientVersion": "5.5.0"
  }
}